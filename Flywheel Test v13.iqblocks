</block></next></block></next></block></next></block></next></block></next></block></xml>","rconfig":[{"port":[9],"name":"motorLeft","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse"},"triportSourcePort":22},{"port":[10],"name":"motorRight","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse"},"triportSourcePort":22},{"port":[3],"name":"intake","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse"}}],"slot":2,"platform":"IQ","sdkVersion":"20211210.18.00.00","appVersion":"2.3.0","fileFormat":"1.0.1","icon":"","targetBrainGen":"First","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START IQ MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END IQ MACROS\n\n\n// Robot configuration code.\nmotor motorLeft = motor(PORT9, false);\nmotor motorRight = motor(PORT10, false);\nmotor intake = motor(PORT3, false);\n\n\n\n// Generated code.\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0;\n\nfloat myVariable, motorsRunning, motorSpeed;\n\nevent message1 = event();\nevent setSpeed = event();\n\n// \"when started\" hat block\nint whenStarted1() {\n  intake.setVelocity(100.0, percent);\n  motorSpeed = 0.0;\n  setSpeed.broadcast();\n  Brain.playSound(tada);\n  Brain.Screen.clearScreen();\n  Brain_precision = 3;\n  Brain.Screen.setCursor(1, 1);\n  Brain.Screen.print(\"All flywheel motors\");\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"connected!\");\n  motorLeft.setVelocity(motorSpeed, percent);\n  motorRight.setVelocity(motorSpeed, percent);\n  motorLeft.setStopping(coast);\n  motorRight.setStopping(coast);\n  wait(2.0, seconds);\n  Brain.Screen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n  Brain.Screen.print(\"Press check to spin,\");\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"arrow buttons for\");\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"direction.\");\n  motorsRunning = 0.0;\n  return 0;\n}\n\n// \"when I receive setSpeed\" hat block\nvoid onevent_setSpeed_0() {\n  // Do not mess with this, as it is calibrated carefully so the flywheel works.\n  motorSpeed = 37.0;\n}\n\n// \"when buttonUp pressed\" hat block\nvoid onevent_buttonUp_pressed_0() {\n  motorLeft.stop();\n  motorRight.stop();\n  wait(3.0, seconds);\n  motorLeft.spin(reverse);\n  motorRight.spin(forward);\n}\n\n// \"when buttonCheck pressed\" hat block\nvoid onevent_buttonCheck_pressed_0() {\n  if (motorsRunning == 0.0) {\n    intake.spin(forward);\n    motorLeft.spin(reverse);\n    motorRight.spin(forward);\n    motorsRunning = 1.0;\n  }\n  else {\n    intake.stop();\n    motorLeft.stop();\n    motorRight.stop();\n    motorsRunning = 0.0;\n    wait(3.0, seconds);\n  }\n}\n\n// \"when buttonDown pressed\" hat block\nvoid onevent_buttonDown_pressed_0() {\n  motorLeft.stop();\n  motorRight.stop();\n  wait(3.0, seconds);\n  motorLeft.spin(forward);\n  motorRight.spin(reverse);\n}\n\n\nint main() {\n  // register event handlers\n  setSpeed(onevent_setSpeed_0);\n  Brain.buttonUp.pressed(onevent_buttonUp_pressed_0);\n  Brain.buttonCheck.pressed(onevent_buttonCheck_pressed_0);\n  Brain.buttonDown.pressed(onevent_buttonDown_pressed_0);\n\n  wait(15, msec);\n  whenStarted1();\n}","target":"Physical"}